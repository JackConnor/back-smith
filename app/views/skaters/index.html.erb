<h1>Skaters#index</h1>
<p>Find me in app/views/skaters/index.html.erb</p>

<div class="skaters-container">
  <ul>
    <% @skaters.each do |s| %>
      <li><img class="index-sktr-photo" src="<%=s.photo%>" alt="" /></li>
      <li><%= s.name %></li>
      <li class="last-blurb"><%= s.city %></li>
    <% end %>
  </ul>

</div>
